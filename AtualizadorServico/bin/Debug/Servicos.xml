<Servicos>
  <Envia_NFe_Email registroWindows="Envia_NFe_Email">
    <RegistroWindows>
      <Envia_NFe_Email>
        <valores>
          <Path_Entrada_NFe>
            <tipo>string</tipo>
            <especifico>diretorio</especifico>
          </Path_Entrada_NFe>
          <Permite_Enviar_Email>
            <tipo>bool</tipo>
          </Permite_Enviar_Email>
          <Path_Certificado_Digital>
            <tipo>string</tipo>
            <especifico>diretorio</especifico>
          </Path_Certificado_Digital>
          <Path_Log_Serv>
            <tipo>string</tipo>
            <especifico>diretorio</especifico>
          </Path_Log_Serv>
          <Path_Log_Xml>
            <tipo>string</tipo>
            <especifico>diretorio</especifico>
          </Path_Log_Xml>
          <Path_Retorno_Txt>
            <tipo>string</tipo>
            <especifico>diretorio</especifico>
          </Path_Retorno_Txt>
          <PathCCE>
            <tipo>string</tipo>
            <especifico>diretorio</especifico>
          </PathCCE>
          <msg_versao>
            <tipo>bool</tipo>
          </msg_versao>
          <versao_sist>
            <tipo>string</tipo>
            <especifico>regex</especifico>
            <validacao>\bv?[0-9]+\.[0-9]+\.[0-9]+(\.[0-9]+)\b</validacao>
            <mensagem>O campo deve ser do tipo versão no formato "0.0.0.0"</mensagem>
          </versao_sist>
          <senha_certificado>
            <tipo>string</tipo>
            <criptografia>true</criptografia>
          </senha_certificado>
        </valores>
      </Envia_NFe_Email>
    </RegistroWindows>
  </Envia_NFe_Email>
